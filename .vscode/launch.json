{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch debug",
        "runtimeExecutable": "${workspaceFolder}/node_modules/nodemon/bin/nodemon.js",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/spacedeck.js",
        "cwd": "${workspaceFolder}",
        "env": {
          "NODE_APP_INSTANCE" :"dev"
        }
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Launch ldap",
        "runtimeExecutable": "${workspaceFolder}/node_modules/nodemon/bin/nodemon.js",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/spacedeck.js",
        "cwd": "${workspaceFolder}",
        "env": {
          "NODE_APP_INSTANCE" :"ldap"
        }
      }
    ]
  }